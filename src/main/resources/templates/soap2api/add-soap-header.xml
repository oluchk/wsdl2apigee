<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XSL async="false" continueOnError="false" enabled="true" name="add-soap-header">
    <DisplayName>Add SOAP Header</DisplayName>
    <Properties/>
    <Source>request</Source>
    <ResourceURL>xsl://add-soap-header.xslt</ResourceURL>
    <Parameters ignoreUnresolvedVariables="true">
        <Parameter name="username" ref="request.header.username"/>
        <Parameter name="password" ref="request.header.password"/>
        <Parameter name="timestamp" ref="timestamp"/>
        <Parameter name="action" ref="soap.action"/>
    </Parameters>
</XSL>